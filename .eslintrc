{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-unused-vars": "off",
      "no-undef": "off",
      "indent": ["error", 4, {"SwitchCase": 1}],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }